<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.esisalama.esisrepo">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EsisRepo"
        tools:targetApi="31">
        <activity
            android:name=".WorkListActivity"
            android:label="@string/workListTitle"
            android:exported="false" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/profileTitle"
            android:exported="false" />
        <activity
            android:name=".AddDocActivity"
            android:label="@string/addDocTitle"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/loginTitle"
            android:exported="false" />
        <activity
            android:name=".HomePageActivity"
            android:exported="true"
            android:label="@string/homeTitle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
